<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.boot.mybatis20220923junho.repository.NewsRepository"> <!-- implement 객체 -->


    <insert id="save" parameterType="com.boot.mybatis20220923junho.domain.News" > <!-- id = 인터페이스의 클래스명과 동일 -->
        insert into
            news_mst
        values (
            0,
            #{news_title},
            #{news_writer},
            #{news_broadcasting},
            #{news_content},
            now(),
            now()
        )
    </insert>
</mapper>